public class Person {
    int attack_range;
    String name;

    public Person(String n, int range) {
        attack_range = range;
        name = n;
    }

    public void profile() {
        System.out.println("\nName: " + name +"\nRange: " + attack_range + "\n");
    }

    public int getAttack_range() {
        return attack_range;
    }

    public String getName() {
        return name;
    }
}
