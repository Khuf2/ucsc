import java.util.Scanner;

public class Runner {

    public static void main(String[] args) {
        // Creation of Scanners, reader for strings and inter for numbers
        Scanner input = new Scanner(System.in);

        Methods caller = new Methods();

        Person user1 = caller.makePerson();

        user1.incrementSkill();

        Enemy enemy1 = caller.makeEnemy("Bummy Ass Nigga");
        enemy1.incrementSkill();

        caller.battle(user1, enemy1);


    }
}
